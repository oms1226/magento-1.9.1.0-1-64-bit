export PHP_PEAR_SYSCONF_DIR
. /home/ubuntu/magento/magento-1.9.1.0-1-64-bit/php/../scripts/setenv.sh
exec /home/ubuntu/magento/magento-1.9.1.0-1-64-bit/php/bin/php.bin "$@"
